{
    // ESLint
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "eslint.validate": ["typescript"],

    // Requires extension: fabiospampinato.vscode-highlight
    "highlight.regexes": {
        "(((jsl|scl)\\.)?unused\\([a-zA-Z_,\\[\\]\\s\\-\":.]+\\);*)": [ // jsl.unused([some_var, some_other_var]); or scl.unused(xy);
            {
                "backgroundColor": "rgba(0, 0, 0, 0.2)",
                "color": "#f88"
            }
        ],
        "(#MARKER)": [ // #MARKER test
            {
                "backgroundColor": "#f41",
                "color": "#fff",
                "isWholeLine": true,
                "overviewRulerColor": "#f41"
            }
        ],
        "(#SECTION ([^\\S\\r\\n]*[\\w,.\\-_]+)*[:]*)": [ // #SECTION test, 123
            {
                "backgroundColor": "#44f",
                "color": "white",
                "overviewRulerColor": "#44f"
            }
        ],
        "(#DEBUG#)": [ // #DEBUG#
            {
                "backgroundColor": "#ff0",
                "color": "blue",
                "overviewRulerColor": "#ff0"
            }
        ],
        "(<!--%#INSERT:[a-zA-Z0-9\\-]+#%-->|<!--%#INJECT:[a-zA-Z0-9\\-]+#%-->)": [ // <!--%#INSERT:TEST#%--> / <!--%#INJECT:TEST#%-->
            {
                "color": "orange",
                "fontStyle": "italic",
                "outline": "1px solid orange"
            }
        ],
        "(<%#INSERT:[a-zA-Z0-9\\-]+#%>|<%#INJECT:[a-zA-Z0-9\\-]+#%>)": [ // <%#INSERT:TEST#%> / <%#INJECT:TEST#%>
            {
                "color": "orange",
                "fontStyle": "italic",
                "outline": "1px solid orange"
            }
        ],
    },

    // Git
    "git.branchPrefix": "feat/",
    "git.branchProtection": [ "main" ],
    "git.branchProtectionPrompt": "alwaysPrompt",
}