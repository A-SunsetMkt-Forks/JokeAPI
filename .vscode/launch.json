    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "JokeAPI",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/JokeAPI.js",
            "args": [
                "--trace-warnings"
            ],
            "console": "integratedTerminal"
        },
        {
            "program": "${workspaceFolder}/tools/test.js",
            "name": "Unit Tests",
            "request": "launch",
            "type": "node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "--trace-deprecation"
            ],
            "console": "integratedTerminal"
        },
        {
            "program": "${workspaceFolder}/tools/info.js",
            "name": "Tools/xy",
            "request": "launch",
            "type": "node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "--trace-deprecation"
            ],
            "console": "integratedTerminal"
        },
        {
            "program": "${workspaceFolder}/tools/cli.js",
            "name": "CLI",
            "request": "launch",
            "type": "node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "--trace-deprecation"
            ],
            "console": "integratedTerminal"
        },
        {
            "program": "${workspaceFolder}/test.js",
            "name": "test.js",
            "request": "launch",
            "type": "node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "--trace-deprecation"
            ],
            "console": "integratedTerminal"
        }
    ],
    "debug.javascript.usePreview": true
}